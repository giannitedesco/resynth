import ipv4;
import dns;

let conn = ipv4::udp::flow(
  192.168.238.112:13749,
  172.16.239.127:53,
);

conn.client_dgram(
    dns::hdr(
      id: 1234,
      opcode: dns::opcode::QUERY,
      qdcount: 1,
    ),
    dns::question(
      qname: dns::name("www", "google", "com"),
      qtype: dns::type::A,
      qclass: dns::class::IN,
    )
);

let server = 142.250.207.36;
dns::host("www.google.com", ns: 8.8.8.8, server);
